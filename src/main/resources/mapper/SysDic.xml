<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shin.pay.dao.SysDicDao">

	<resultMap id="SysDic" type="com.shin.pay.entity.SysDic" >
		<id property="id" column="ID" javaType="java.lang.Long" />
		<result property="code" column="CODE" />
		<result property="title" column="TITLE" />
	</resultMap>
	
	<select id="getDics" resultMap="SysDic" parameterType="Object" >
		select code, short_name title from sys_dic where dic_type = #{dicType}
	</select>

</mapper>